{
  "name": "phonebook_backend",
  "version": "0.0.1",
  "description": "phonebook_backend",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js --ignore tests/",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "build:ui": "rm -rf build && cd ../fullstack/part3/the_phonebook && npm run build --prod && cp -r build ../../../fullstack_back_end",
    "deploy": "git push heroku phonebook_backend:master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy",
    "restart:heroku": "heroku restart",
    "logs:prod": "heroku logs --tail",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinelikeamillion/fullstack.git"
  },
  "keywords": [
    "phonebook_backend"
  ],
  "author": "Sean Matro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinelikeamillion/fullstack/issues"
  },
  "homepage": "https://github.com/shinelikeamillion/fullstack#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.18",
    "mongoose": "^5.9.21",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
